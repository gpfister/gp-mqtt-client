{
  "name": "gp-mqtt-client",
  "version": "1.0.0",
  "description": "Simple MQTT Client (no auth, decode base64, ...)",
  "main": "lib/gp-mqtt-client.js",
  "bin": {
    "gp-mqtt-client": "lib/gp-mqtt-client.js"
  },
  "scripts": {
    "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "build": "tsc --sourceMap",
    "test": "mocha --require ts-node/register ./test/test.ts --timeout 10000"
  },
  "engines": {
    "node": ">=14.0.0 <20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gpfister/gp-mqtt-client.git"
  },
  "keywords": [
    "MQTT",
    "client",
    "Mosquitto",
    "base64",
    "json"
  ],
  "author": "Greg PFISTER",
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/gpfister/gp-mqtt-client/issues"
  },
  "homepage": "https://github.com/gpfister/gp-mqtt-client#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "log4js": "^6.9.1",
    "mqtt": "^4.3.7",
    "uuid": "^9.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "@types/uuid": "^9.0.1",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "chai": "^4.3.7",
    "eslint": "^8.39.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^4.9.5"
  }
